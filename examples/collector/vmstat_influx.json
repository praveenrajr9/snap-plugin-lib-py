{
  "version": 1,
  "log_level": 2,
  "log_path": "/tmp/",
  "schedule": {
    "type": "simple",
    "interval": "1s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/vmstats/pgfault_per_itv": {},
        "/intel/vmstats/pgfree_per_itv": {},
        "/intel/vmstats/pgpgin_per_itv": {},
        "/intel/vmstats/pgpgout_per_itv": {},
        "/intel/vmstats/pgscan_kswapd_per_itv": {},
        "/intel/vmstats/pgscan_direct_per_itv": {},
        "/intel/vmstats/pgsteal_per_itv": {},
        "/intel/vmstats/vmeff%": {}

      },

      "process": null,
      "publish": [
        {
           "plugin_name": "influxdb",
           "config": {
            "host": "localhost",
            "port": 8086,
            "database": "vmemory_metrics",
            "user": "intel",
            "password": "intel123"

          }

 
        }
      ]
    }
  }
}
