{
  "version": 1,
  "log_level": 2,
  "log_path": "/tmp/",
  "schedule": {
    "type": "simple",
    "interval": "2s"
  },
  "workflow": {
    "collect": {
      "metrics": {
	"/intel/storageIOstats/device/*/r_await": {},
	"/intel/storageIOstats/device/sda/writes_per_itv": {},
	"/intel/storageIOstats/device/sda/wrsec_per_itv": {}

      },

      "process": null,
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/vmstat"
          }
        }
      ]
    }
  }
}
