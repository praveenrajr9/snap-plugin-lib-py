{
  "version": 1,
  "log_level": 2,
  "log_path": "/tmp/",
  "schedule": {
    "type": "simple",
    "interval": "1s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/sys/kernel/block_trace/*/block": {},
	"/intel/sys/kernel/block_trace/*/bytes": {},
	"/intel/sys/kernel/block_trace/*/command": {},
	"/intel/sys/kernel/block_trace/*/dev": {},
	"/intel/sys/kernel/block_trace/*/end": {},
	"/intel/sys/kernel/block_trace/*/lat": {},
	"/intel/sys/kernel/block_trace/*/pid": {},
	"/intel/sys/kernel/block_trace/*/start": {},
	"/intel/sys/kernel/block_trace/*/type": {}
      },

      "process": null,
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/vmstat"
          }
        }
      ]
    }
  }
}
