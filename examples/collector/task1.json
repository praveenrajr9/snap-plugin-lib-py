{
  "version": 1,
  "log_level": 2,
  "log_path": "/tmp/",
  "schedule": {
    "type": "simple",
    "interval": "1s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/vmstats/pgfault_per_itv": {},
        "/intel/vmstats/pgfree_per_itv": {},
        "/intel/vmstats/pgpgin_per_itv": {},
        "/intel/vmstats/pgpgout_per_itv": {},
        "/intel/vmstats/pgscan_kswapd_per_itv": {},
        "/intel/vmstats/pgscan_direct_per_itv": {},
        "/intel/vmstats/pgsteal_per_itv": {},
        "/intel/vmstats/vmeff%": {},
	"/intel/storageIOstats/device/*/rdsec_per_itv": {},
	"/intel/storageIOstats/device/*/reads_per_itv": {},
	"/intel/storageIOstats/device/*/transfers_per_itv": {},
	"/intel/storageIOstats/device/*/util": {},
	"/intel/storageIOstats/device/*/wrsec_per_itv": {}

      },

      "process": null,
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/vmstat"
          }
        }
      ]
    }
  }
}
